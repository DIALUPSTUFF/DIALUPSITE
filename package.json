{
  "name": "dialupsite",
  "version": "1.0.0",
  "description": "A website for Dial Up, a small collective that I'm a part of. We DJ, write, and make music if we can. This website is for displaying our online magazines and other forms of conent. Generally though, we just want a space to be as creative as we want with the design/technology on this site!",
  "main": "server.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "start": "npm-run-all -p watch-css start-js",
    "build": "npm-run-all build-css build-js",
    "start-js": "NODE_ENV=production webpack -p && NODE_ENV=production node src/server.js",
    "build-js": "NODE_ENV=production webpack -p",
    "build-css": "node-sass-chokidar src/ -o src/",
    "watch-css": "npm run build-css && node-sass-chokidar src/ -o src/ --watch --recursive",
    "server": "webpack -d && node src/server.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/DIALUPSTUFF/DialUpSite.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/DIALUPSTUFF/DIALUPSITE/issues"
  },
  "homepage": "https://github.com/DIALUPSTUFF/DIALUPSITE#readme",
  "dependencies": {
    "babel-cli": "^6.11.4",
    "babel-core": "^6.13.2",
    "babel-loader": "^6.2.8",
    "babel-preset-es2015": "^6.13.2",
    "babel-preset-react": "^6.11.1",
    "classnames": "^2.2.5",
    "colornames": "^1.1.1",
    "consolidate": "^0.14.5",
    "css-loader": "^0.26.0",
    "detect-browser": "^1.5.0",
    "dotenv": "^8.2.0",
    "express": "^4.14.0",
    "express-robots": "^0.1.6",
    "extract-text-webpack-plugin": "^1.0.1",
    "firebase": "^7.2.3",
    "marklar": "0.0.5",
    "mustache": "^2.3.0",
    "node-sass-chokidar": "^1.3.3",
    "npm-run-all": "^4.1.3",
    "path": "^0.12.7",
    "pm2": "^3.0.3",
    "react": "^15.3.2",
    "react-dom": "^15.3.2",
    "react-flexbox-grid": "^1.1.5",
    "react-inlinesvg": "^0.8.2",
    "react-player": "^0.14.2",
    "react-router": "^2.6.1",
    "react-twitch-embed-video": "^1.0.1",
    "socket.io-client": "^1.7.2",
    "style-loader": "^0.13.1",
    "url-loader": "^0.5.7",
    "velocity-react": "^1.4.1"
  },
  "devDependencies": {
    "node-sass": "^4.13.0",
    "sass-loader": "^8.0.0",
    "webpack": "^1.15.0",
    "webpack-dev-server": "^3.1.11"
  }
}
